{
  "compilerOptions": {
    "isolatedModules": false,
    "outDir": "build",
    "lib": [
      "ESNext"
    ],
    "esModuleInterop": false,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "sourceMap": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true
  },
}